RewriteEngine On

# IMPORTANTE: ajustá si tu proyecto no es /animalios/public
RewriteBase /animalios/public/

# Si existe archivo o carpeta, servilo normal
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

# Si no existe, mandá todo a index.php
RewriteRule ^ index.php [L]